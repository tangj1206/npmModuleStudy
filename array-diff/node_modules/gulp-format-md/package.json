{
  "name": "gulp-format-md",
  "description": "Gulp plugin for beautifying markdown using pretty-remarkable.",
  "version": "0.1.12",
  "homepage": "https://github.com/jonschlinkert/gulp-format-md",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-format-md",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-format-md/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "pretty-remarkable": "^0.4.1",
    "remarkable": "^1.7.1",
    "sections": "^0.1.10",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.0",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "beautify",
    "format",
    "formatter",
    "gulp",
    "gulpplugin",
    "markdown",
    "md",
    "plugin",
    "prettify",
    "pretty",
    "remarkable"
  ],
  "verb": {
    "related": {
      "list": [
        "gulp",
        "pretty-remarkable",
        "remarkable"
      ]
    },
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "./"
    ],
    "toc": false,
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb"
    ]
  }
}
